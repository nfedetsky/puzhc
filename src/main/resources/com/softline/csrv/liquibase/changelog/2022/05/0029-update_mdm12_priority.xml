<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.5.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">

    <changeSet id="0029-update_mdm12_priority_1" author="IKretov">
        <update tableName="mdm12_priority">
            <column name="sort_order" value="1"/>
            <where> code = 'LOW'</where>
        </update>
    </changeSet>
    <changeSet id="0029-update_mdm12_priority_2" author="IKretov">
        <update tableName="mdm12_priority">
            <column name="sort_order" value="2"/>
            <where> code = 'MIDDLE'</where>
        </update>
    </changeSet>
    <changeSet id="0029-update_mdm12_priority_3" author="IKretov">
        <update tableName="mdm12_priority">
            <column name="sort_order" value="3"/>
            <where> code = 'HIGH'</where>
        </update>
    </changeSet>
    <changeSet id="0029-update_mdm12_priority_4" author="IKretov">
        <update tableName="mdm12_priority">
            <column name="sort_order" value="4"/>
            <where> code = 'CRITICAL'</where>
        </update>
    </changeSet>
    <changeSet id="0029-update_mdm12_priority_5" author="IKretov">
        <update tableName="mdm12_priority">
            <column name="sort_order" value="5"/>
            <where> code = 'BLOCKED'</where>
        </update>
    </changeSet>

</databaseChangeLog>