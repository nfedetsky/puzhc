<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.5.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="0021-1320_upt_mdm19-1" author="csrv">
        <update tableName="mdm19_request_type">
            <column name="ICON_PATH" value="SPINNER"/>
            <where>code = 'AGREEMENT'</where>
        </update>
        <update tableName="mdm19_request_type">
            <column name="ICON_PATH" value="CC"/>
            <where>code = 'CONTENT_AGREEMENT'</where>
        </update>
    </changeSet>
    <changeSet id="0021-1320_upt_mdm19-2" author="csrv">
        <update tableName="mdm19_request_type">
            <column name="ICON_PATH" value="FILE_O"/>
            <where>code = 'CONTRACT'</where>
        </update>

    </changeSet>


</databaseChangeLog>